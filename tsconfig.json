{
  "compilerOptions": {
    "jsx": "preserve",
    "target": "ES5",
    "lib": [
      "ES2015",
      "DOM"
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/login/businessLicense.jsx",
    "components/forms/profileEditForm/profileEditForm.tsx",
    // "middleware.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}